		text = new JLabel("Select a category");
	}
	
	// ------------------------------- Commands ---------------------------------------- //
	
	public void set(){
		gui.add(text);
	}
	
	
	public void close(){
		
	}
	// ------------------------------- Queries ----------------------------------------- //
	
}
